{
    "manifest_version": 2,
    "name": "Bookmark Storer",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>" 
            ],
            "js": ["content.js"]
        }
    ],
    "permissions": ["tabs", "<all_urls>"],
    
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Store bookmarks & notes here!",
        "default_popup": "popup.html?popup=true"
    }
}
